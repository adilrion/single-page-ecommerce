{
    "name": "ecommerce-backend",
    "version": "1.0.0",
    "description": "Node.js TypeScript backend for e-commerce SPA",
    "main": "dist/index.js",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "dev": "ts-node-dev --respawn --transpile-only -r tsconfig-paths/register src/index.ts",
        "lint": "eslint src/**/*.ts",
        "lint:fix": "eslint src/**/*.ts --fix",
        "test": "jest",
        "seed": "ts-node src/scripts/seed.ts"
    },
    "keywords": [
        "nodejs",
        "typescript",
        "express",
        "mongodb",
        "ecommerce"
    ],
    "author": "Your Name",
    "license": "MIT",
    "dependencies": {
        "bcryptjs": "^2.4.3",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-async-errors": "^3.1.1",
        "express-rate-limit": "^6.10.0",
        "helmet": "^7.0.0",
        "joi": "^17.9.2",
        "jsonwebtoken": "^9.0.2",
        "mongoose": "^7.5.0",
        "morgan": "^1.10.0",
        "uuid": "^11.1.0"
    },
    "devDependencies": {
        "@types/bcryptjs": "^2.4.4",
        "@types/compression": "^1.7.3",
        "@types/cors": "^2.8.14",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.4",
        "@types/jsonwebtoken": "^9.0.2",
        "@types/morgan": "^1.9.5",
        "@types/node": "^20.5.0",
        "@types/uuid": "^10.0.0",
        "@typescript-eslint/eslint-plugin": "^6.4.0",
        "@typescript-eslint/parser": "^6.4.0",
        "eslint": "^8.47.0",
        "jest": "^29.6.2",
        "ts-jest": "^29.1.1",
        "ts-node": "^10.9.1",
        "ts-node-dev": "^2.0.0",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.1.6"
    },
    "engines": {
        "node": ">=16.0.0"
    }
}
